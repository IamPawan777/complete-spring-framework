package com.spring;

public class Student {
	private int id;
	private int roll;
	
	public Student(int id, int roll) {
		this.id = id;
		this.roll = roll;
		System.out.println("int | string");
	}
	
	public Student(double roll, double id) {
		this.roll = (int)roll;
		this.id = (int)id;
		System.out.println("String | int");
	}
	
	public Student(String roll, String id) {
		this.roll = Integer.parseInt(roll);
		this.id = Integer.parseInt(id);
		System.out.println("String | string");
	}

	@Override
	public String toString() {
		return "Student : id is= " + id + " || name is= " + name;
	}
	
}
